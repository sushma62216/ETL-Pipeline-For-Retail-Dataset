{
    "name": "Data Engineering ETL",
    "image": "mcr.microsoft.com/devcontainers/python:3.9",
    "features": {
      "ghcr.io/devcontainers/features/mysql:1": {}
    },
    "forwardPorts": [3306],
    "postCreateCommand": "bash setup.sh && pip install -r requirements.txt",
